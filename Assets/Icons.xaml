<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Router Icon -->
    <Canvas x:Key="RouterIcon" Width="24" Height="24">
        <Path Data="M 2,16 L 22,16 L 22,8 L 2,8 L 2,16 Z M 4,10 L 20,10 L 20,14 L 4,14 L 4,10 Z" 
              Fill="Black" />
        <Path Data="M 7,14 L 7,10 M 10,14 L 10,10 M 13,14 L 13,10 M 16,14 L 16,10" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 8,6 L 16,6 L 16,8 M 8,18 L 16,18 L 16,16" 
              Stroke="Black" StrokeThickness="1" />
    </Canvas>
    
    <!-- Modem Icon -->
    <Canvas x:Key="ModemIcon" Width="24" Height="24">
        <Path Data="M 4,10 L 20,10 L 20,18 L 4,18 L 4,10 Z M 6,12 L 18,12 L 18,16 L 6,16 L 6,12 Z" 
              Fill="Black" />
        <Path Data="M 7,16 L 7,12 M 10,16 L 10,12" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 14,12 C 14,12 14,16 14,16" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 16,12 C 16,12 16,16 16,16" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 8,6 C 8,6 8,10 8,10 M 16,6 C 16,6 16,10 16,10" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 8,6 C 8,6 16,6 16,6" 
              Stroke="Black" StrokeThickness="1" />
    </Canvas>
    
    <!-- Switch Icon -->
    <Canvas x:Key="SwitchIcon" Width="24" Height="24">
        <Path Data="M 2,6 L 22,6 L 22,18 L 2,18 L 2,6 Z M 4,8 L 20,8 L 20,16 L 4,16 L 4,8 Z" 
              Fill="Black" />
        <Path Data="M 5,9 L 6,9 M 5,10.5 L 6,10.5 M 5,12 L 6,12 M 5,13.5 L 6,13.5 M 5,15 L 6,15" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 18,9 L 19,9 M 18,10.5 L 19,10.5 M 18,12 L 19,12 M 18,13.5 L 19,13.5 M 18,15 L 19,15" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 8,10 L 16,10 M 8,12 L 16,12 M 8,14 L 16,14" 
              Stroke="Black" StrokeThickness="1" />
    </Canvas>
    
    <!-- Server Icon -->
    <Canvas x:Key="ServerIcon" Width="24" Height="24">
        <Path Data="M 4,4 L 20,4 L 20,20 L 4,20 L 4,4 Z M 5,5 L 19,5 L 19,9 L 5,9 L 5,5 Z M 5,10 L 19,10 L 19,14 L 5,14 L 5,10 Z M 5,15 L 19,15 L 19,19 L 5,19 L 5,15 Z" 
              Fill="Black" />
        <Path Data="M 6,7 L 8,7 M 6,12 L 8,12 M 6,17 L 8,17" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 17,7 L 18,7 M 17,12 L 18,12 M 17,17 L 18,17" 
              Stroke="Black" StrokeThickness="1" />
    </Canvas>
    
    <!-- PC Icon -->
    <Canvas x:Key="PCIcon" Width="24" Height="24">
        <Path Data="M 3,6 L 17,6 L 17,16 L 3,16 L 3,6 Z M 4,7 L 16,7 L 16,15 L 4,15 L 4,7 Z" 
              Fill="Black" />
        <Path Data="M 8,16 L 12,16 L 12,18 L 17,18 L 17,19 L 3,19 L 3,18 L 8,18 L 8,16 Z" 
              Fill="Black" />
    </Canvas>
    
    <!-- Laptop Icon -->
    <Canvas x:Key="LaptopIcon" Width="24" Height="24">
        <Path Data="M 4,7 L 20,7 L 20,15 L 4,15 L 4,7 Z M 5,8 L 19,8 L 19,14 L 5,14 L 5,8 Z" 
              Fill="Black" />
        <Path Data="M 2,18 L 22,18 L 20,15 L 4,15 L 2,18 Z" 
              Fill="Black" />
    </Canvas>
    
    <!-- Smartphone Icon -->
    <Canvas x:Key="SmartphoneIcon" Width="24" Height="24">
        <Path Data="M 8,4 L 16,4 L 16,20 L 8,20 L 8,4 Z M 9,6 L 15,6 L 15,18 L 9,18 L 9,6 Z" 
              Fill="Black" />
        <Path Data="M 11,5 L 13,5" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 12,19 C 12,19 12,19 12,19" 
              Stroke="Black" StrokeThickness="2" />
    </Canvas>
    
    <!-- IoT Device Icon -->
    <Canvas x:Key="IoTDeviceIcon" Width="24" Height="24">
        <Path Data="M 6,8 L 18,8 L 18,16 L 6,16 L 6,8 Z M 7,9 L 17,9 L 17,15 L 7,15 L 7,9 Z" 
              Fill="Black" />
        <Path Data="M 5,5 C 5,5 5,19 5,19 M 19,5 C 19,5 19,19 19,19" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 9,12 C 9,12 9,12 9,12 M 12,12 C 12,12 12,12 12,12 M 15,12 C 15,12 15,12 15,12" 
              Stroke="Black" StrokeThickness="1.5" />
        <Path Data="M 3,3 L 21,3 L 21,21 L 3,21 L 3,3 Z M 4,4 L 20,4 L 20,20 L 4,20 L 4,4 Z" 
              Fill="Black" />
    </Canvas>
    
    <!-- Connection Icon -->
    <Canvas x:Key="ConnectionIcon" Width="24" Height="24">
        <Path Data="M 4,12 L 20,12" 
              Stroke="Black" StrokeThickness="2" />
        <Path Data="M 17,9 L 20,12 L 17,15" 
              Stroke="Black" StrokeThickness="2" StrokeLineJoin="Round" StrokeStartLineCap="Round" StrokeEndLineCap="Round" />
        <Path Data="M 7,9 L 4,12 L 7,15" 
              Stroke="Black" StrokeThickness="2" StrokeLineJoin="Round" StrokeStartLineCap="Round" StrokeEndLineCap="Round" />
    </Canvas>
    
    <!-- Ethernet Icon -->
    <Canvas x:Key="EthernetIcon" Width="24" Height="24">
        <Path Data="M 4,12 L 20,12" 
              Stroke="Black" StrokeThickness="2" />
        <Path Data="M 6,8 L 8,8 L 8,16 L 6,16 L 6,8 Z M 16,8 L 18,8 L 18,16 L 16,16 L 16,8 Z" 
              Fill="Black" />
    </Canvas>
    
    <!-- WiFi Icon -->
    <Canvas x:Key="WiFiIcon" Width="24" Height="24">
        <Path Data="M 12,18 C 12,18 12,18 12,18" 
              Stroke="Black" StrokeThickness="2" />
        <Path Data="M 8,14 C 9.5,12.5 14.5,12.5 16,14" 
              Stroke="Black" StrokeThickness="2" StrokeStartLineCap="Round" StrokeEndLineCap="Round" />
        <Path Data="M 5,11 C 7.5,8.5 16.5,8.5 19,11" 
              Stroke="Black" StrokeThickness="2" StrokeStartLineCap="Round" StrokeEndLineCap="Round" />
        <Path Data="M 2,8 C 5.5,4.5 18.5,4.5 22,8" 
              Stroke="Black" StrokeThickness="2" StrokeStartLineCap="Round" StrokeEndLineCap="Round" />
    </Canvas>
    
    <!-- Fiber Optic Icon -->
    <Canvas x:Key="FiberOpticIcon" Width="24" Height="24">
        <Path Data="M 4,12 L 20,12" 
              Stroke="Black" StrokeThickness="2" StrokeDashArray="1,1" />
        <Path Data="M 6,8 L 9,8 L 9,16 L 6,16 L 6,8 Z M 15,8 L 18,8 L 18,16 L 15,16 L 15,8 Z" 
              Fill="Black" />
    </Canvas>
    
    <!-- USB Icon -->
    <Canvas x:Key="USBIcon" Width="24" Height="24">
        <Path Data="M 10,4 L 14,4 L 14,8 L 15,8 L 15,12 L 9,12 L 9,8 L 10,8 L 10,4 Z" 
              Fill="Black" />
        <Path Data="M 9,12 L 15,12 L 15,20 L 9,20 L 9,12 Z M 10,13 L 14,13 L 14,19 L 10,19 L 10,13 Z" 
              Fill="Black" />
        <Path Data="M 11,6 L 13,6" 
              Stroke="Black" StrokeThickness="1" />
    </Canvas>
    
    <!-- Bluetooth Icon -->
    <Canvas x:Key="BluetoothIcon" Width="24" Height="24">
        <Path Data="M 12,4 L 12,20 M 8,8 L 16,16 M 8,16 L 16,8" 
              Stroke="Black" StrokeThickness="2" />
    </Canvas>
    
    <!-- Add Icon -->
    <Canvas x:Key="AddIcon" Width="24" Height="24">
        <Path Data="M 12,4 L 12,20 M 4,12 L 20,12" 
              Stroke="Black" StrokeThickness="2" />
    </Canvas>
    
    <!-- Delete Icon -->
    <Canvas x:Key="DeleteIcon" Width="24" Height="24">
        <Path Data="M 6,6 L 18,18 M 6,18 L 18,6" 
              Stroke="Black" StrokeThickness="2" />
    </Canvas>
    
    <!-- Settings Icon -->
    <Canvas x:Key="SettingsIcon" Width="24" Height="24">
        <Path Data="M 10,4 L 14,4 L 14,5 C 14,5 15,5.5 16,6 L 17,5 L 19,7 L 18,8 C 18,8 19,9 19,10 L 20,10 L 20,14 L 19,14 C 19,15 18,16 18,16 L 19,17 L 17,19 L 16,18 C 15,19 14,19 14,19 L 14,20 L 10,20 L 10,19 C 9,19 8,18 8,18 L 7,19 L 5,17 L 6,16 C 5,15 5,14 5,14 L 4,14 L 4,10 L 5,10 C 5,9 6,8 6,8 L 5,7 L 7,5 L 8,6 C 9,5 10,5 10,5 L 10,4 Z M 12,8 C 9.8,8 8,9.8 8,12 C 8,14.2 9.8,16 12,16 C 14.2,16 16,14.2 16,12 C 16,9.8 14.2,8 12,8 Z" 
              Fill="Black" />
    </Canvas>
    
    <!-- Simulation Icon -->
    <Canvas x:Key="SimulationIcon" Width="24" Height="24">
        <Path Data="M 4,12 L 20,12 M 4,8 L 20,8 M 4,16 L 20,16" 
              Stroke="Black" StrokeThickness="1" />
        <Path Data="M 12,6 C 12,6 16,12 16,12 C 16,12 12,18 12,18 M 8,6 C 8,6 12,12 12,12 C 12,12 8,18 8,18" 
              Stroke="Black" StrokeThickness="1" />
    </Canvas>
    
    <!-- Plugin Icon -->
    <Canvas x:Key="PluginIcon" Width="24" Height="24">
        <Path Data="M 14,4 L 18,4 L 18,8 L 14,8 L 14,4 Z M 6,10 L 10,10 L 10,14 L 6,14 L 6,10 Z M 14,10 L 18,10 L 18,14 L 14,14 L 14,10 Z M 10,16 L 14,16 L 14,20 L 10,20 L 10,16 Z" 
              Fill="Black" />
        <Path Data="M 12,4 L 12,8 M 6,12 L 10,12 M 18,12 L 14,12 M 12,16 L 12,20" 
              Stroke="Black" StrokeThickness="1" />
    </Canvas>
</ResourceDictionary>
